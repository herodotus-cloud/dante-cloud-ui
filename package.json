{
	"name": "herodotus-cloud-ui",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"serve": "vite preview",
		"build": "vite build --mode production",
		"dev": "vite --force --host --mode development",
		"preview": "vite preview",
		"push": "yarn commit && git push --set-upstream origin vuetify3",
		"commit": "git add . && git status && git-cz"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"husky-run": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.(js|json|md|scss|ts|vue)": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@mdi/font": "7.4.47",
		"@vee-validate/i18n": "^4.13.2",
		"@vee-validate/rules": "^4.13.2",
		"animate.css": "^4.1.1",
		"axios": "^1.7.4",
		"colord": "^2.9.3",
		"js-base64": "^3.7.7",
		"localforage": "^1.10.0",
		"lodash-es": "^4.17.21",
		"moment": "^2.30.1",
		"nprogress": "^0.2.0",
		"particles.vue3": "^2.12.0",
		"pinia": "^2.2.1",
		"pinia-plugin-persistedstate": "^3.2.1",
		"qs": "^6.13.0",
		"roboto-fontface": "^0.10.0",
		"sm-crypto": "^0.3.13",
		"sweetalert2": "^11.12.4",
		"tsparticles": "^3.5.0",
		"vee-validate": "^4.13.2",
		"vue": "^3.4.37",
		"vue-i18n": "^9.13.1",
		"vue-router": "^4.4.3",
		"vuetify": "^3.6.14",
		"webfontloader": "^1.6.28"
	},
	"devDependencies": {
		"@types/lodash-es": "^4.17.12",
		"@types/qs": "^6.9.15",
		"@types/sm-crypto": "^0.3.4",
		"@types/webfontloader": "^1.6.38",
		"@typescript-eslint/eslint-plugin": "^8.1.0",
		"@typescript-eslint/parser": "^8.1.0",
		"@vitejs/plugin-vue": "^5.1.2",
		"@vue/compiler-sfc": "^3.4.37",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"dotenv": "^16.4.5",
		"eslint": "^9.9.0",
		"eslint-plugin-vue": "^9.27.0",
		"husky": "^9.1.4",
		"lint-staged": "^15.2.9",
		"prettier": "^3.3.3",
		"sass": "^1.77.8",
		"sass-loader": "^16.0.0",
		"typescript": "^5.5.4",
		"unplugin-icons": "^0.19.2",
		"unplugin-vue-components": "^0.27.4",
		"vite": "^5.4.0",
		"vite-plugin-vuetify": "^2.0.4",
		"vue-cli-plugin-vuetify": "~2.5.8",
		"vue-tsc": "^2.0.29",
		"vuetify-loader": "2.0.0-alpha.9",
		"webpack": "^5.93.0"
	}
}
