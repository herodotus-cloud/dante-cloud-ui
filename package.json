{
  "name": "dante-cloud-ui",
  "private": true,
  "version": "3.4.2.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "commit": "git add . && git status && git-cz",
    "push": "pnpm run commit && git push --set-upstream origin develop",
    "dev": "pnpm run --filter=@dante/ui dev",
    "prod": "pnpm run --filter=@dante/ui prod",
    "shared:build": "pnpm -r --filter=@herodotus/* run build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky-run": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|json|md|scss|ts|vue)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@quasar/extras": "^1.16.16",
    "disable-devtool": "^0.3.8",
    "lodash-es": "^4.17.21",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "qs": "^6.14.0",
    "quasar": "^2.17.7",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@quasar/vite-plugin": "^1.9.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.0",
    "@types/qs": "^6.9.18",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-quasar": "^1.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.1",
    "postcss-html": "^1.8.0",
    "prettier": "^3.4.2",
    "rollup": "4.34.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.83.4",
    "stylelint": "^16.14.1",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "terser": "^5.37.0",
    "typescript": "^5.7.3",
    "unocss": "^65.4.3",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-icons": "^22.0.0",
    "unplugin-vue-components": "^28.0.0",
    "vconsole": "^3.15.1",
    "vite": "^6.0.11",
    "vite-plugin-compression2": "^1.3.3",
    "vite-plugin-dts": "4.5.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-vconsole": "^2.1.1",
    "vue-tsc": "^2.2.0"
  }
}
